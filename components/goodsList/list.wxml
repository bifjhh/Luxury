<template name="goodsList">
  <view class="goods_box">
    <view class="goods" wx:for="{{goods_list}}" wx:key="index">
      <view class="goods_img">
        <image src="{{item.goods_img}}" />
      </view>
      <view class="goods_h1">
        <text class="money"><text>￥</text>{{item.price}}</text>
        <text class="zk">¥{{item.original_cost}}</text>
        <text class="jj" wx:if="{{item.is_sales}}">降</text>
        <text class="fq" wx:if="{{item.is_credit==1}}">分期</text>

      </view>
      <view class="goods_zk">
        [{{item.quality_name}}]{{item.goods_title}}
      </view>
    </view>
  </view>
</template>