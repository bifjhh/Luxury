view {
  box-sizing: border-box;
}

.nav_tab {
  height: 80rpx;
  padding: 0 25rpx;
  background-color: #fff;
}

.tab_box {
  height: 80rpx;
  width: auto;
  white-space: nowrap;
}

.tab_box view {
  display: inline-block;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  position: relative;
  padding: 0 25rpx;
}

.nav_tab view.action {
  color: rgba(173, 155, 106, 1);
}

.nav_tab .action::after {
  position: absolute;
  content: '';
  width: 40rpx;
  height: 4rpx;
  background: rgba(173, 155, 106, 1);
  border-radius: 2px;
  left: 50%;
  bottom: 2px;
  transform: translateX(-50%);
}

.indent_box {
  margin-top: 2px;
}

.indent {
  /* height: 336rpx; */
  display: flex;
  align-content: center;
  justify-content: center;
  flex-direction: column;
  padding: 12rpx 30rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}

.haoma {
  display: flex;
  align-content: center;
  justify-content: space-between;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.logo {
  margin-top: 30rpx;
  height: 150rpx;
  display: flex;
  /* border-bottom: 1px dashed #999; */
  overflow: hidden;
  /* justify-content: center; */
}

.logo_img {
  width: 150rpx;
  height: 150rpx;
}

.logo_info {
  margin-left: 20rpx;
  width: 410rpx;
  height: 85rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.num {
  margin-top: 30rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.num_l>text {
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
}

.num_r {
  width: 175rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 25px;
  font-family: PingFang-SC-Bold;
}

.num_r>view {
  font-size: 26rpx;
  color: rgba(255, 255, 255, 1);
}

.null_img{
  width: 255rpx;
  height: 205rpx;
  overflow: hidden;
  margin: 0 auto;
  margin-top: 150rpx;
}
.null_img image{
  width: 100%;
  height: 100%;
}
.null_text{
  color: #666666;
  font-size: 30rpx;
  margin-top: 20rpx;
  text-align: center;
}