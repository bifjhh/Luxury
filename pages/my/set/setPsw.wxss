
page,view{
  box-sizing: border-box;
}
page{
 background-color: #fff;
 padding: 0 30rpx;
}

.list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
}
.btn{
  margin: 0 auto;
  margin-top: 100rpx;
  height: 80rpx;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  border-radius: 40rpx;
  font-size: 32rpx;
  width: 600rpx;
}
.huming>text{
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(51,51,51,1);
  line-height:40rpx;
  width: 196rpx;
}
input{
  margin-left: 1em;
  flex: 1;
  font-size:28rpx;
  font-family:PingFang-SC-Medium;
  line-height:40rpx;
  padding-left: 1em;
  border-bottom: #eee 1px solid;
}
