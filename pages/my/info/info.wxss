page>view {
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 30rpx;
  border-bottom: 1px solid #f2f2f2;
  height: 100rpx;
}

.head {
  height: 134rpx;
}

page>view>view {
  display: flex;
  align-items: center;
  justify-content: center;
}

.head .image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
}

.jt {
  width: 13rpx;
  height: 24rpx;
  margin-left: 36rpx;
}

.login {
  display: block;
  border: none;
  height: 80rpx;
  position: fixed;
  width: 100%;
  bottom: 80rpx;
  left: 0;
  background: none;
}

.login>view {
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  height: 100%;
  border-radius: 40px;
  background: rgb(223, 223, 223);
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(153, 153, 153, 1);
}

.gender>view {
  height: 100%;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.gender>view picker {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.gender>view .picker {
  width: 100%;
  height: 100%;
}