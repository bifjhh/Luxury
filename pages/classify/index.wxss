page{
  background: #fff;
}
image{
  width: 100%;
  height: 100%;
}
.title{
  display: flex;
  width: 100%;
  height: 88rpx;
  align-items: center;
  justify-content: center;
  border-bottom: 2px solid rgba(236,238,238,1);
}
.title_info{
  font-size:36rpx;
  color:rgba(51, 51, 51, 1);
  line-height: 44rpx;
  position: relative;
}
.title_br::after{
  position: absolute;
  content: '';
  width: 40rpx;
  height: 4rpx;
  background: rgba(173,155,106,1);
  bottom: -16rpx;
  left: 50%;
  transform: translateX(-50%);
}

.brand{
  box-sizing: border-box;
  text-align: center;
  padding: 0 30rpx;
  display: flex;
  flex-wrap:wrap;
  justify-content: center;
}
.brand_info{
  width: 25%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.brand_img{
  width: 150rpx;
  height: 150rpx;
}
.brand_h1{
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(51, 51, 51, 1);
  line-height:40rpx;
}
.container {
  position: relative;
  width: 100%;
  background-color: #fff;
  color: #939393;
  display: flex;
  flex-direction:row;
}
.nav_left{
  display: inline-block;
  width: 200rpx;
  background: #f5f5f5;
  text-align: center;
  box-sizing: border-box;
}
.nav_left .nav_left_items{
  height: 100rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #dedede;
  font-size:28rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(51, 51, 51, 1);
  display: flex;
  align-items: center;
  justify-content: center;
  background:rgba(255, 255, 255, 1);
}
.nav_left .nav_left_items.active{
  border-left: 10rpx solid rgba(173,155,106,1);
  background:rgba(246, 246, 246, 1);
  font-size:28rpx;
  font-family:PingFang-SC-Bold;
  color:rgba(173, 155, 106, 1);
  line-height:40rpx;
}
.nav_right{
  flex: 1;
  background: #fff;
  box-sizing: border-box;
}
.nav_right .nav_logo{
  width: 143rpx;
  height: 180rpx;
  text-align: center;
  float:left;
  margin-top: 30rpx;
  margin-left: 30rpx;
}
.nav_img{
  width: 100%;
  height: 143rpx;
}
 .nav_logo .nav_txt {
  display: block;
  margin-top: 14rpx;
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(102, 102, 102, 1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}