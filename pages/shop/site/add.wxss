page{
  background-color: #fff;
}
view{
  box-sizing: border-box;
}
.box>view{
  padding: 0 30rpx;
  border-bottom: 1px solid #f2f2f2;
  height: 100rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.box>view>view{
  width: 180rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(51,51,51,1);
}
input{
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(51,51,51,1);
}
.jinse{
  width: 690rpx;
  height: 80rpx;
  margin: 0 auto;
  border-radius:40px;
  font-size:34rpx;
  font-family:PingFang-SC-Bold;
  color:rgba(255,255,255,1);
  line-height:80rpx;
}