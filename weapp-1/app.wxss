.container {
    font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
}

page {
    background-color: #F2f2f2;
}

view,
text {
    font-size: 24rpx;
}

image {
    width: 100%;
    height: 100%;
}

.flex_column {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}
.t20{
    margin-top: 20rpx;
    background: #fff;
}