page {
  background: #fff;
}

view {
  box-sizing: border-box;
}

.add {
  height: 142rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(25, 138, 243, 1);
  line-height: 40rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #f2f2f2;
  padding: 0 30rpx;
}

.number {
  padding: 38rpx 30rpx;
  border-bottom: 1px solid #f2f2f2;
}

.number>text {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.input {
  margin-top: 40rpx;
  font-size: 72rpx;
  font-family: PingFang-SC-Medium;
  line-height: 40rpx;
  color: #333;
}
input{
  width: 100%;
  height: 100%;
}
.take_btn {
  width: 690rpx;
  height: 80rpx;
  margin: 0 auto;
  margin-top: 111rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 40rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Bold;
  color: rgba(255, 255, 255, 1);
  line-height: 49rpx;
}

.ts {
  margin-top: 20rpx;
  font-size: 24epx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
  line-height: 40rpx;
  text-align: right;
  padding-right: 1em;
}
.keyong{
  margin-top: 15rpx;
  padding-left: 1em;
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(153,153,153,1);
  line-height:40rpx;
}

.yhk{
  padding: 0 30rpx;
  height: 142rpx;
  border-bottom:1px solid #f2f2f2;
  display: flex;
  align-items: center;
}
.logo_img{
  width: 90rpx;
  height: 90rpx;
  display: flex;
  align-items: center; 
  
}
.logo_img>image{
  margin: 0 auto;
}
.yhk_info{
  margin-left: 20rpx;
  display:flex;
  align-items:initial;
  flex-direction: column;
}
.yhk_info>text{
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(51, 51, 51, 1);
  line-height:40rpx;
}
.yhk_info>text:nth-of-type(2){
  font-size:24rpx;
}