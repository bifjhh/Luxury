.container {
    background-color: #F2f2f2;
}
image{
    width: 100%;
    height: 100%;
}
.swiper_box {
    width: 100%;
    height: 482rpx;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
}
.xuanze{
    display: flex;
    width: 100%;
    height: 180rpx;
    background: #fff;
    margin-top: 20rpx;

}
.xuanze>view{
    text-align: center;
    flex: auto;
    display: flex;
    align-items:center;
    justify-content:center;
    flex-direction:column;
}
.xuanze_img{
    width: 50rpx;
    height: 50rpx;
}
.xuanze_text{
    
    font-size:24rpx; 
    font-family:PingFang-SC-Medium;
    color:rgba(51,51,51,1);
    line-height:40px;
}


.fr{
    float: right;
}
.fl{
    float: left;
}



.venues_box {
    padding: 0 10rpx;
    display: flex;
    background: #fff;
    margin-top: 20rpx;
}
.venues_box>view{
    box-sizing: border-box;
    flex: auto;
    width: 50%;
    padding-left: 40rpx;
}


.venues_content{
    width: 100%;
    height: 200rpx;
    display: flex;
    align-items:center;
}

.v_img{
    width: 150rpx;
    height: 150rpx;
    /* background: #333; */
}


.v_info{
    flex: 1;
}

.v_h1{
    font-size:28rpx; 
    font-family:PingFang-SC-Bold;
    color:rgba(51,51,51,1);
    line-height:40rpx;
}
.v_text{
    font-size:22rpx; 
    font-family:PingFang-SC-Medium;
    color:rgba(153,153,153,1);
    line-height:40rpx;
}

.venues_content_max{
    width: 100%;
    padding-top: 43rpx;
}
.v_img_max{
    margin-top: 50rpx;
    height: 212rpx;
}
