page{
  padding-bottom: 120rpx;
}
view{
  box-sizing: border-box;
}
.dizhi{
  height: 88rpx;
  border-bottom: 1px solid #f2f2f2;
  display: flex;
  align-items: center;
  background: #fff;
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(51,51,51,1);
  padding-left: 32rpx;
  
}


.isadd {
  height: 160rpx;
  background: #fff;
   padding: 0 30rpx;
   display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: inherit;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 50rpx;
  padding-right: 50rpx;
}


.add_dz {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}

.jt {
  padding: 0 !important;
  margin: 0 !important;
  width: 19rpx;
  height: 33rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}

.name_tel>.tel {
  margin-left: 2em;
}

.addres {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.goods_list {
  padding: 0 44rpx 0 30rpx;
}

.goods {
  width: 100%;
  height: 277rpx;
  display: flex;
  align-items: center;
  padding: 39rpx 0;
  border-bottom: 1px solid #f2f2f2;
}

.goods_logo {
  width: 198rpx;
  height: 198rpx;
}

.goods_info {
  width: 445rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 45rpx;
  position: relative;
  margin-left: 33rpx;
  height: 100%;
}

.info_txt {
  position: absolute;
  left: 0;
  top: 0;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.goods_info .money {
  position: absolute;
  left: 0;
  bottom: 0;
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(255, 114, 114, 1);
}

.curt {
  position: absolute;
  bottom: 0;
  right: 0;
  font-size: 26rpx;
  font-family: Helvetica;
  color: rgba(51, 51, 51, 1);
}


.money>view {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  padding: 0 68rpx 0 30rpx;
  border-bottom: 1px solid #f2f2f2;
}

.mn_num {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
}


.money>view {
    padding: 0 30rpx;
}
.isnum{
  color:rgba(133, 84, 36, 1);
}
.money text{
  color: #333;
}
.submit{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background: #fff;
  height: 100rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;

}

.submit_btn{
  flex: 1;
  height: 70rpx;
  border-radius:35px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:32rpx;
  font-family:PingFang-SC-Bold;
  color:rgba(255, 255, 255, 1);
}

.lxkf{
  padding: 0;
  margin-top: 44rpx;
  width: 240rpx;
  height: 60rpx;
  border-radius:10px;
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(102,102,102,1);
  line-height:40px;
  border:1px solid #f2f2f2;
}