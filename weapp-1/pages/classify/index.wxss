page{
  background: #f5f5f5;
}
image{
  width: 100%;
  height: 100%;
}
.title{
  display: flex;
  width: 100%;
  height: 88rpx;
  align-items: center;
  justify-content: center;
}
.title_info{
  font-size:36rpx;
  color:rgba(51, 51, 51, 1);
  line-height: 44rpx;
}
.brand{
  box-sizing: border-box;
  text-align: center;
  padding: 0 30rpx;
  display: flex;
  flex-wrap:wrap;
  justify-content: center;
}
.brand_info{
  width: 25%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.brand_img{
  width: 150rpx;
  height: 150rpx;
}
.brand_h1{
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  color:rgba(51, 51, 51, 1);
  line-height:40rpx;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}
.nav_left{
  display: inline-block;
  width: 25%;
  height: 100%;
  background: #f5f5f5;
  text-align: center;
}
.nav_left .nav_left_items{
  height: 30px;
  line-height: 30px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 14px;
}
.nav_left .nav_left_items.active{
  background: #fff;
}
.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 75%;
  height: 100%;
  padding: 10px;
  background: #fff;
  box-sizing: border-box;
}
.nav_right .nav_right_items{
  float: left;
  width: 33.33%;
  height: 80px;
  text-align: center;
}
.nav_right .nav_right_items image{
  width: 50px;
  height: 30px;
}
.nav_right .nav_right_items text{
  display: block;
  margin-top: 5px;
  font-size: 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}